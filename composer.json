{
    "name": "vring/douyin-openapi",
    "description": "对接抖音开放平台的sdk",
    "license": "MIT",
    "keywords": [
        "php",
        "sdk",
        "douyin",
        "douyin-open",
        "douyin-open-sdk"
    ],
    "autoload": {
        "psr-4": {
            "vring\\DouyinOpenapi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "vring",
            "email": "971626354@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0|^8.0",
        "ydg/foudation-sdk": "^1.0",
        "ext-json": "*",
        "symfony/cache": "*"
    },

    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "cs-fix": "php-cs-fixer fix ."
    }
}
